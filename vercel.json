{
  "rewrites": [
    {
      "source": "/modules/(.*)",
      "destination": "/modules/$1/index.html"
    }
  ],
  "headers": [
    {
      "source": "/modules/(.*)/index.html",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html"
        }
      ]
    }
  ]
}